  function startMovie(element)
 {
         //make active or inactive
         $(".movieLink").removeClass("active");
         $(element).addClass("active");

        $("#movieloadingspinner").show();

 }
  

 function onMovieFrameLoad()
 {
           $("#movieloadingspinner").hide();
 }
  


</script>
